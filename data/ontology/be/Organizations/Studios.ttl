@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix fibo-be-le-lei: <https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalEntities/> .
@prefix fibo-be-le-fbo: <https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/FormalBusinessOrganizations/> .
@prefix lop-core: <https://lop.com/fibo/lifestyle/core/> .
@prefix lop-be-org: <https://lop.com/fibo/lifestyle/be/org/> .

<https://lop.com/fibo/lifestyle/be/Organizations/Studios> a owl:Ontology ;
    owl:imports <https://lop.com/fibo/lifestyle/md/LOPFitness-Metadata> ;
    rdfs:label "LOP Studios"@en .

lop-core:WellnessStudio a owl:Class ;
    rdfs:subClassOf fibo-be-le-fbo:BusinessDivision ;
    skos:definition "LOP 협업 스튜디오"@ko .

lop-be-org:hasRegion a owl:DatatypeProperty ;
    rdfs:domain lop-core:WellnessStudio ;
    rdfs:range <http://www.w3.org/2001/XMLSchema#string> .

lop-be-org:supportsProgram a owl:ObjectProperty ;
    rdfs:domain lop-core:WellnessStudio ;
    rdfs:range lop-core:FitnessProgram .

lop-core:SGANG01 a lop-core:WellnessStudio ;
    rdfs:label "서울 강남 스튜디오"@ko ;
    lop-be-org:hasRegion "Seoul" ;
    lop-be-org:supportsProgram lop-core:FitnessPlusKickoff .

lop-core:SBRCH02 a lop-core:WellnessStudio ;
    rdfs:label "부산 리커버리 허브"@ko ;
    lop-be-org:hasRegion "Busan" ;
    lop-be-org:supportsProgram lop-core:FitnessPlusSeniors .
