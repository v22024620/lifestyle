@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix fibo-loan-loan: <https://spec.edmcouncil.org/fibo/ontology/LOAN/Loans/> .
@prefix lop-core: <https://lop.com/fibo/lifestyle/core/> .
@prefix lop-loan: <https://lop.com/fibo/lifestyle/loan/> .

<https://lop.com/fibo/lifestyle/loan/StudioCapexLoans> a owl:Ontology ;
    owl:imports <https://lop.com/fibo/lifestyle/md/LOPFitness-Metadata> .

lop-loan:StudioCapexLoan a owl:Class ;
    rdfs:subClassOf fibo-loan-loan:LoanContract ;
    skos:definition "스튜디오 장비 투자 대출"@ko .

lop-loan:fundsStudio a owl:ObjectProperty ;
    rdfs:domain lop-loan:StudioCapexLoan ;
    rdfs:range lop-core:WellnessStudio .

lop-loan:CapexLoan-Base a lop-loan:StudioCapexLoan ;
    rdfs:label "SGANG01 CAPEX"@ko ;
    lop-loan:fundsStudio lop-core:SGANG01 .
