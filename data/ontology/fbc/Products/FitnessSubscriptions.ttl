@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix fibo-fbc-pas-fpas: <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> .
@prefix lop-core: <https://lop.com/fibo/lifestyle/core/> .
@prefix lop-fbc-prod: <https://lop.com/fibo/lifestyle/fbc/products/> .

<https://lop.com/fibo/lifestyle/fbc/Products/FitnessSubscriptions> a owl:Ontology ;
    owl:imports <https://lop.com/fibo/lifestyle/md/LOPFitness-Metadata> .

lop-core:FitnessSubscriptionProduct a owl:Class ;
    rdfs:subClassOf fibo-fbc-pas-fpas:ServiceOffering ;
    skos:definition "LOP+ 구독 상품"@ko .

lop-fbc-prod:hasTier a owl:DatatypeProperty ;
    rdfs:domain lop-core:FitnessSubscriptionProduct ;
    rdfs:range <http://www.w3.org/2001/XMLSchema#string> .

lop-core:FitnessPlusPremium a lop-core:FitnessSubscriptionProduct ;
    rdfs:label "Fitness+ Premium"@en ;
    lop-fbc-prod:hasTier "Premium" .
