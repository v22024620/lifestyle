@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix fibo-fbc-fi-fi: <https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/> .
@prefix lop-core: <https://lop.com/fibo/lifestyle/core/> .
@prefix lop-fbc-set: <https://lop.com/fibo/lifestyle/fbc/settlement/> .

<https://lop.com/fibo/lifestyle/fbc/Accounting/FitnessSettlement> a owl:Ontology ;
    owl:imports <https://lop.com/fibo/lifestyle/md/LOPFitness-Metadata> .

lop-core:SettlementProfile a owl:Class ;
    rdfs:subClassOf fibo-fbc-fi-fi:SettlementTerms ;
    skos:definition "Revenue share, 정산 빈도"@ko .

lop-fbc-set:hasRevenueShare a owl:DatatypeProperty ;
    rdfs:domain lop-core:SettlementProfile ;
    rdfs:range <http://www.w3.org/2001/XMLSchema#decimal> .

lop-core:SettlementProfile-Std a lop-core:SettlementProfile ;
    rdfs:label "표준 정산"@ko ;
    lop-fbc-set:hasRevenueShare "0.30"^^<http://www.w3.org/2001/XMLSchema#decimal> .
