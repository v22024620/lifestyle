@prefix apple: <https://lop.com/fibo/lifestyle/apple/> .
@prefix lop-be-org: <https://lop.com/fibo/lifestyle/be/org/> .
@prefix lop-core: <https://lop.com/fibo/lifestyle/core/> .
@prefix lop-der: <https://lop.com/fibo/lifestyle/der/> .
@prefix lop-ext-ind: <https://lop.com/fibo/lifestyle/extensions/individuals/> .
@prefix lop-ext-prog: <https://lop.com/fibo/lifestyle/extensions/programs/> .
@prefix lop-fbc-prod: <https://lop.com/fibo/lifestyle/fbc/products/> .
@prefix lop-ind: <https://lop.com/fibo/lifestyle/ind/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

apple:Alignment-APPLEFIT01 a lop-ext-prog:StudioProgramAlignment ;
    lop-ext-prog:alignmentProgram lop-core:FitnessPlusInfinity ;
    lop-ext-prog:alignmentStudio lop-core:APPLEFIT01 .

apple:Evidence-APPLEFIT01 a lop-ext-ind:DerivedEvidence ;
    rdfs:label "APPLEFIT01 Evidence"@en ;
    apple:applePlannerNote "워치 링 로열티 코호트는 스튜디오 정규 세션의 68%를 차지"@ko ;
    apple:cohortTag "Watch-Ring-Loyalists" ;
    lop-ext-ind:referencesMetric apple:AppleWatchEngagementMomentum ;
    lop-ext-ind:referencesStudio lop-core:APPLEFIT01 .

apple:applePlannerNote a owl:DatatypeProperty ;
    rdfs:domain lop-ext-ind:DerivedEvidence ;
    rdfs:range xsd:string .

apple:cohortTag a owl:DatatypeProperty ;
    rdfs:domain lop-ext-ind:DerivedEvidence ;
    rdfs:range xsd:string .

apple:subscriptionBlend a owl:DatatypeProperty ;
    rdfs:domain lop-core:WellnessStudio ;
    rdfs:range xsd:string .

apple:watchRingDailyRatio a owl:DatatypeProperty ;
    rdfs:domain lop-core:WellnessStudio ;
    rdfs:range xsd:decimal .

lop-core:AppleOnePremierBundle a lop-core:FitnessSubscriptionProduct ;
    rdfs:label "Apple One Premier Bundle"@en ;
    lop-fbc-prod:hasTier "Premier" .

lop-ind:RiskEvent-APPLEFIT01-Churn a lop-ind:FitnessRiskEvent ;
    rdfs:label "워치 링 정체로 인한 구독 이탈 위험"@ko ;
    lop-ext-ind:evidenceURI "https://lop.com/evidence/applefit01/churn"^^xsd:anyURI ;
    lop-ext-ind:referencesMetric lop-der:ChurnRiskIndex ;
    lop-ext-ind:referencesStudio lop-core:APPLEFIT01 .

apple:AppleWatchEngagementMomentum a lop-der:DerivedMetric ;
    rdfs:label "Apple Watch Engagement Momentum"@en ;
    rdfs:subClassOf lop-der:DerivedMetric ;
    skos:definition "Watch 링 연속 달성률의 가속도"@ko .

lop-core:FitnessPlusInfinity a lop-core:FitnessProgram ;
    rdfs:label "Fitness+ Infinity Flow"@en ;
    skos:definition "워치 기반 하이브리드 근력/마음챙김"@ko .

lop-core:APPLEFIT01 a lop-core:WellnessStudio ;
    rdfs:label "애플 피트니스+ 플래그십 강남"@ko ;
    schema1:identifier "APPLEFIT01" ;
    apple:subscriptionBlend "Apple One Premier 72% | Fitness+ Direct 28%" ;
    apple:watchRingDailyRatio 0.93 ;
    lop-be-org:hasRegion "Seoul" ;
    lop-be-org:supportsProgram lop-core:FitnessPlusInfinity ;
    lop-ext-ind:evidenceScore 0.96 .

