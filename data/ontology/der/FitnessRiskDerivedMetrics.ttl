@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix fibo-ind-ei: <https://spec.edmcouncil.org/fibo/ontology/IND/EconomicIndicators/EconomicIndicators/> .
@prefix lop-core: <https://lop.com/fibo/lifestyle/core/> .
@prefix lop-der: <https://lop.com/fibo/lifestyle/der/> .

<https://lop.com/fibo/lifestyle/der/FitnessRiskDerivedMetrics> a owl:Ontology ;
    owl:imports <https://lop.com/fibo/lifestyle/md/LOPFitness-Metadata> .

lop-core:KPISet-Base a owl:NamedIndividual .

lop-der:DerivedMetric a owl:Class ;
    rdfs:subClassOf fibo-ind-ei:EconomicIndicator ;
    skos:definition "LOP 파생 KPI"@ko .

lop-der:ActivationMomentum a lop-der:DerivedMetric ;
    rdfs:label "Activation Momentum"@en ;
    skos:definition "연속 4주 활성화율 가속도"@ko .

lop-der:ChurnRiskIndex a lop-der:DerivedMetric ;
    skos:definition "환불/비활성률 기반 리스크 지수"@ko .

lop-der:feedsKPISet a owl:ObjectProperty ;
    rdfs:domain lop-der:DerivedMetric ;
    rdfs:range lop-core:KPISet .

lop-der:ActivationMomentum lop-der:feedsKPISet lop-core:KPISet-Base .
