@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix lop-core: <https://lop.com/fibo/lifestyle/core/> .
@prefix lop-bp: <https://lop.com/fibo/lifestyle/bp/> .

<https://lop.com/fibo/lifestyle/bp/Engagements/ProgramLifecycle> a owl:Ontology ;
    owl:imports <https://lop.com/fibo/lifestyle/md/LOPFitness-Metadata> .

lop-core:FitnessProgram a owl:Class ;
    rdfs:label "LOP Program"@en ;
    skos:definition "LOP+에서 파트너와 실행하는 프로그램"@ko .

lop-bp:ProgramLifecycleEvent a owl:Class ;
    rdfs:subClassOf <http://www.w3.org/2006/time#Instant> ;
    skos:definition "프로그램 라이프사이클 이벤트"@ko .

lop-bp:hasLifecycleEvent a owl:ObjectProperty ;
    rdfs:domain lop-core:FitnessProgram ;
    rdfs:range lop-bp:ProgramLifecycleEvent .

lop-core:FitnessPlusKickoff a lop-core:FitnessProgram ;
    rdfs:label "피트니스 플러스 킥오프"@ko .

lop-bp:KickoffApproval a lop-bp:ProgramLifecycleEvent ;
    rdfs:label "Kickoff 승인"@ko .

lop-core:FitnessPlusKickoff lop-bp:hasLifecycleEvent lop-bp:KickoffApproval .
