@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fibo-life: <https://example.com/fibo/lifestyle#> .
@prefix fibo-fbc-pas-caa: <https://spec.edmcouncil.org/fibo/FBC/PAS/CreditCardsAndAccounts/> .
@prefix fibo-fbc-pas-tx: <https://spec.edmcouncil.org/fibo/FBC/PAS/Transactions/> .
@prefix fibo-fbc-ins-fun: <https://spec.edmcouncil.org/fibo/FBC/Insurance/InsuranceContracts/> .
@prefix fibo-be-le-lp: <https://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/> .
@prefix fibo-fnd-agr-ctr: <https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/> .
@prefix fibo-fnd-acc-cur: <https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/> .
@prefix fibo-fnd-qt-qtu: <https://spec.edmcouncil.org/fibo/FND/Quantities/QuantitiesAndUnits/> .
@prefix fibo-fnd-rel-rel: <https://spec.edmcouncil.org/fibo/FND/Relations/Relations/> .

<https://example.com/fibo/lifestyle-extension>
    a owl:Ontology ;
    owl:imports <https://example.com/fibo/core-subset> ;
    dcterms:title "FIBO Lifestyle Extension for LOP Partnerships"@en ;
    dcterms:creator "LOP planning team"@en ;
    dcterms:description "Aligns Bloomberg FIBO legal/transaction concepts with Korean wellness studios participating in LOP."@en .

fibo-life:WellnessStudio a owl:Class ;
    rdfs:label "LOP-aligned wellness studio"@en ;
    rdfs:subClassOf fibo-be-le-lp:LegalEntity .

fibo-life:Trainer a owl:Class ;
    rdfs:label "Partner trainer"@en ;
    rdfs:subClassOf fibo-be-le-lp:LegalEntity .

fibo-life:WellnessProgram a owl:Class ;
    rdfs:label "Wellness program"@en ;
    rdfs:subClassOf fibo-fnd-agr-ctr:ContractualAgreement .

fibo-life:LOPFitnessAlignment a owl:Class ;
    rdfs:label "LOP alignment contract"@en ;
    rdfs:subClassOf fibo-fnd-agr-ctr:ContractualAgreement .

fibo-life:ProgramParticipationAgreement a owl:Class ;
    rdfs:label "Program participation agreement"@en ;
    rdfs:subClassOf fibo-fnd-agr-ctr:PerformanceCommitment .

fibo-life:EngagementMeasurement a owl:Class ;
    rdfs:label "Engagement measurement"@en ;
    rdfs:subClassOf fibo-fnd-qt-qtu:Measurement .

fibo-life:FitnessRiskEvent a owl:Class ;
    rdfs:label "Fitness program risk event"@en ;
    rdfs:subClassOf fibo-fbc-pas-tx:Transaction .

fibo-life:ChargebackEvent a owl:Class ;
    rdfs:label "Fitness chargeback"@en ;
    rdfs:subClassOf fibo-fbc-pas-tx:Chargeback .

fibo-life:RefundEvent a owl:Class ;
    rdfs:label "Fitness refund"@en ;
    rdfs:subClassOf fibo-fbc-pas-tx:Refund .

fibo-life:UsageSettlementInstruction a owl:Class ;
    rdfs:label "Usage settlement instruction"@en ;
    rdfs:subClassOf fibo-fbc-pas-tx:SettlementInstruction .

fibo-life:lopFitnessTier a owl:DatatypeProperty ;
    rdfs:label "LOP tier"@en ;
    rdfs:domain fibo-life:WellnessStudio ;
    rdfs:range xsd:string .

fibo-life:lopCommerceTier a owl:DatatypeProperty ;
    rdfs:label "LOP commerce tier"@en ;
    rdfs:domain fibo-life:WellnessStudio ;
    rdfs:range xsd:string .

fibo-life:supportsFitnessPlusPrograms a owl:DatatypeProperty ;
    rdfs:label "Fitness+ program coverage"@en ;
    rdfs:domain fibo-life:WellnessStudio ;
    rdfs:range xsd:string .

fibo-life:lopMetricSyncLevel a owl:DatatypeProperty ;
    rdfs:label "LOP metric sync level"@en ;
    rdfs:domain fibo-life:WellnessStudio ;
    rdfs:range xsd:string .

fibo-life:studioRiskCategory a owl:DatatypeProperty ;
    rdfs:label "Studio risk category"@en ;
    rdfs:domain fibo-life:WellnessStudio ;
    rdfs:range xsd:string .

fibo-life:alignsWithContract a owl:ObjectProperty ;
    rdfs:label "aligns with contract"@en ;
    rdfs:domain fibo-life:EngagementMeasurement ;
    rdfs:range fibo-fnd-agr-ctr:ContractualAgreement .

fibo-life:reportedForStudio a owl:ObjectProperty ;
    rdfs:label "reported for studio"@en ;
    rdfs:domain fibo-life:EngagementMeasurement ;
    rdfs:range fibo-life:WellnessStudio .

fibo-life:tracksProgram a owl:ObjectProperty ;
    rdfs:label "tracks program"@en ;
    rdfs:domain fibo-life:ProgramParticipationAgreement ;
    rdfs:range fibo-life:WellnessProgram .

fibo-life:hasPrimaryTrainer a owl:ObjectProperty ;
    rdfs:label "has primary trainer"@en ;
    rdfs:domain fibo-life:WellnessStudio ;
    rdfs:range fibo-life:Trainer .

fibo-life:usesSettlementAccount a owl:ObjectProperty ;
    rdfs:label "uses settlement account"@en ;
    rdfs:domain fibo-life:WellnessStudio ;
    rdfs:range fibo-fbc-pas-caa:Account .

fibo-life:sharesHealthDataWith a owl:ObjectProperty ;
    rdfs:label "shares health data with"@en ;
    rdfs:domain fibo-life:WellnessStudio ;
    rdfs:range fibo-be-le-lp:LegalEntity .

fibo-life:lopRingCompletionRate a owl:DatatypeProperty ;
    rdfs:label "LOP ring completion rate"@en ;
    rdfs:domain fibo-life:EngagementMeasurement ;
    rdfs:range xsd:decimal .

fibo-life:lopWatchHeartRateVariance a owl:DatatypeProperty ;
    rdfs:label "LOP Watch heart rate variance"@en ;
    rdfs:domain fibo-life:EngagementMeasurement ;
    rdfs:range xsd:decimal .

fibo-life:watchActiveEnergyDelta a owl:DatatypeProperty ;
    rdfs:label "Active energy delta"@en ;
    rdfs:domain fibo-life:EngagementMeasurement ;
    rdfs:range xsd:decimal .

fibo-life:healthKitOptInRate a owl:DatatypeProperty ;
    rdfs:label "HealthKit opt-in rate"@en ;
    rdfs:domain fibo-life:EngagementMeasurement ;
    rdfs:range xsd:decimal .

fibo-life:fitnessPlusActivationRate a owl:DatatypeProperty ;
    rdfs:label "Fitness+ activation rate"@en ;
    rdfs:domain fibo-life:EngagementMeasurement ;
    rdfs:range xsd:decimal .

fibo-life:LOPFitnessDivision a fibo-be-le-lp:LegalEntity ;
    rdfs:label "LOP Partnerships"@en .

# --- Programs ---

