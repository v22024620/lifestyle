@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fibo-fbc-pas-caa: <https://spec.edmcouncil.org/fibo/FBC/PAS/CreditCardsAndAccounts/> .
@prefix fibo-fbc-pas-tx: <https://spec.edmcouncil.org/fibo/FBC/PAS/Transactions/> .
@prefix fibo-fbc-ins-fun: <https://spec.edmcouncil.org/fibo/FBC/Insurance/InsuranceContracts/> .
@prefix fibo-be-le-lp: <https://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/> .
@prefix fibo-fnd-agr-ctr: <https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/> .
@prefix fibo-fnd-rel-rel: <https://spec.edmcouncil.org/fibo/FND/Relations/Relations/> .
@prefix fibo-fnd-acc-cur: <https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/> .
@prefix fibo-fnd-qt-qtu: <https://spec.edmcouncil.org/fibo/FND/Quantities/QuantitiesAndUnits/> .
@prefix fibo-fnd-aap-ppl: <https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/People/> .

<https://example.com/fibo/core-subset>
    a owl:Ontology ;
    dcterms:title "FIBO Core Subset for Lifestyle Commerce"@en ;
    dcterms:creator "Bloomberg FIBO subset for LOP planning"@en ;
    dcterms:description "Minimal copy of key FIBO classes/properties used by the Lifestyle pipeline"@en .

fibo-be-le-lp:LegalEntity a owl:Class ;
    rdfs:label "Legal entity"@en ;
    dcterms:description "Party recognized in law that can enter contracts and own accounts."@en .

fibo-fnd-aap-ppl:Person a owl:Class ;
    rdfs:label "Person"@en ;
    rdfs:subClassOf fibo-be-le-lp:LegalEntity .

fibo-fnd-agr-ctr:ContractualAgreement a owl:Class ;
    rdfs:label "Contractual agreement"@en .

fibo-fnd-agr-ctr:PerformanceCommitment a owl:Class ;
    rdfs:label "Performance commitment"@en ;
    rdfs:subClassOf fibo-fnd-agr-ctr:ContractualAgreement .

fibo-fbc-pas-caa:Account a owl:Class ;
    rdfs:label "Financial account"@en ;
    rdfs:subClassOf fibo-fnd-agr-ctr:ContractualAgreement .

fibo-fbc-pas-caa:hasAccountHolder a owl:ObjectProperty ;
    rdfs:label "has account holder"@en ;
    rdfs:domain fibo-fbc-pas-caa:Account ;
    rdfs:range fibo-be-le-lp:LegalEntity .

fibo-fbc-pas-caa:hasServicingParty a owl:ObjectProperty ;
    rdfs:label "has servicing party"@en ;
    rdfs:domain fibo-fbc-pas-caa:Account ;
    rdfs:range fibo-be-le-lp:LegalEntity .

fibo-fbc-pas-caa:hasBalance a owl:ObjectProperty ;
    rdfs:label "has balance"@en ;
    rdfs:domain fibo-fbc-pas-caa:Account ;
    rdfs:range fibo-fnd-acc-cur:MonetaryAmount .

fibo-fbc-pas-tx:Transaction a owl:Class ;
    rdfs:label "Financial transaction"@en ;
    dcterms:description "Exchange of value recorded on an account."@en .

fibo-fbc-pas-tx:Chargeback a owl:Class ;
    rdfs:label "Chargeback"@en ;
    rdfs:subClassOf fibo-fbc-pas-tx:Transaction .

fibo-fbc-pas-tx:Refund a owl:Class ;
    rdfs:label "Refund"@en ;
    rdfs:subClassOf fibo-fbc-pas-tx:Transaction .

fibo-fbc-pas-tx:SettlementInstruction a owl:Class ;
    rdfs:label "Settlement instruction"@en ;
    rdfs:subClassOf fibo-fbc-pas-tx:Transaction .

fibo-fbc-pas-tx:hasTransactionDate a owl:DatatypeProperty ;
    rdfs:label "transaction date"@en ;
    rdfs:domain fibo-fbc-pas-tx:Transaction ;
    rdfs:range xsd:dateTime .

fibo-fbc-pas-tx:amount a owl:ObjectProperty ;
    rdfs:label "has monetary amount"@en ;
    rdfs:domain fibo-fbc-pas-tx:Transaction ;
    rdfs:range fibo-fnd-acc-cur:MonetaryAmount .

fibo-fbc-pas-tx:involvesAccount a owl:ObjectProperty ;
    rdfs:label "involves account"@en ;
    rdfs:domain fibo-fbc-pas-tx:Transaction ;
    rdfs:range fibo-fbc-pas-caa:Account .

fibo-fbc-pas-tx:hasCounterparty a owl:ObjectProperty ;
    rdfs:label "has counterparty"@en ;
    rdfs:domain fibo-fbc-pas-tx:Transaction ;
    rdfs:range fibo-be-le-lp:LegalEntity .

fibo-fnd-acc-cur:MonetaryAmount a owl:Class ;
    rdfs:label "Monetary amount"@en .

fibo-fnd-acc-cur:hasAmount a owl:DatatypeProperty ;
    rdfs:label "numeric amount"@en ;
    rdfs:domain fibo-fnd-acc-cur:MonetaryAmount ;
    rdfs:range xsd:decimal .

fibo-fnd-acc-cur:hasCurrency a owl:DatatypeProperty ;
    rdfs:label "currency code"@en ;
    rdfs:domain fibo-fnd-acc-cur:MonetaryAmount ;
    rdfs:range xsd:string .

fibo-fnd-qt-qtu:Measurement a owl:Class ;
    rdfs:label "Measurement"@en .

fibo-fnd-qt-qtu:hasValue a owl:DatatypeProperty ;
    rdfs:label "measurement value"@en ;
    rdfs:domain fibo-fnd-qt-qtu:Measurement ;
    rdfs:range xsd:decimal .

fibo-fnd-qt-qtu:hasUnit a owl:DatatypeProperty ;
    rdfs:label "measurement unit"@en ;
    rdfs:domain fibo-fnd-qt-qtu:Measurement ;
    rdfs:range xsd:string .

fibo-fnd-qt-qtu:hasObservationDate a owl:DatatypeProperty ;
    rdfs:label "observation date"@en ;
    rdfs:domain fibo-fnd-qt-qtu:Measurement ;
    rdfs:range xsd:date .

fibo-fnd-rel-rel:hasFormalRelationship a owl:ObjectProperty ;
    rdfs:label "has formal relationship"@en ;
    rdfs:domain fibo-be-le-lp:LegalEntity ;
    rdfs:range fibo-fnd-agr-ctr:ContractualAgreement .

fibo-fbc-ins-fun:InsurancePolicy a owl:Class ;
    rdfs:label "Insurance policy"@en ;
    rdfs:subClassOf fibo-fnd-agr-ctr:ContractualAgreement .

fibo-fbc-ins-fun:coversParty a owl:ObjectProperty ;
    rdfs:label "carries coverage for"@en ;
    rdfs:domain fibo-fbc-ins-fun:InsurancePolicy ;
    rdfs:range fibo-be-le-lp:LegalEntity .

fibo-fnd-agr-ctr:hasEffectiveDate a owl:DatatypeProperty ;
    rdfs:label "effective date"@en ;
    rdfs:domain fibo-fnd-agr-ctr:ContractualAgreement ;
    rdfs:range xsd:date .

fibo-fnd-agr-ctr:hasExpirationDate a owl:DatatypeProperty ;
    rdfs:label "expiration date"@en ;
    rdfs:domain fibo-fnd-agr-ctr:ContractualAgreement ;
    rdfs:range xsd:date .

fibo-fnd-agr-ctr:hasCounterparty a owl:ObjectProperty ;
    rdfs:label "has contract counterparty"@en ;
    rdfs:domain fibo-fnd-agr-ctr:ContractualAgreement ;
    rdfs:range fibo-be-le-lp:LegalEntity .
